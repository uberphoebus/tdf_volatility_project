   "outputs": [],
   "source": [
    "INTERVAL_WINDOW = 20\n",
    "n_future = 5\n",
    "\n",
    "# GET BACKWARD LOOKING REALIZED VOLATILITY\n",
    "df['vol_current'] = df['HV']\n",
    "# df['stdev_current'] = df['stdev']                                   \n",
    "\n",
    "# GET FORWARD LOOKING REALIZED VOLATILITY \n",
    "df['vol_future'] = df['HV'].shift(-n_future)\n",
    "# df['stdev_future'] = df['stdev'].shift(-n_future)"
   ]
  },
