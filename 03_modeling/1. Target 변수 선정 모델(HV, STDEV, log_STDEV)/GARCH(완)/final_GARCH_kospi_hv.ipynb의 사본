  {
   "cell_type": "code",
   "execution_count": 82,
   "metadata": {},
   "outputs": [],
   "source": [
    "df['Change']    = 100*df['Close'].pct_change()\n",
    "df['STDEV']     = df['Close'].rolling(window=20).std(ddof=0)\n",
    "df['STDEV_log'] = np.log(df['Close'].rolling(window=20).std(ddof=0))\n",
    "df['HV']        = df['Change'].rolling(window=20, center=False).std() * (252 ** 0.5)\n",
    "df['log_returns'] = np.log(df.Close/df.Close.shift(1))"
   ]
  },
